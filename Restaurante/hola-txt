<svg
    id="plano"
    xmlns="http://www.w3.org/2000/svg"
    viewBox="0 0 800 400"
    width="100%"
    height="100%"
    preserveAspectRatio="xMinYMin meet"
    style="background:#f3f3f3;"
>
    <style>
        .wall {
            fill: none;
            stroke: #333;
            stroke-width: 4;
        }
        .bar {
            fill: #444;
            stroke: #222;
            stroke-width: 2;
        }
        .text {
            font-family: sans-serif;
            font-size: 14px;
            fill: #333;
        }
        .spawn-area {
            fill: #ddd;
            stroke: #aaa;
            stroke-dasharray: 6;
            stroke-width: 2;
        }
    </style>

    <!-- ZONA DE SPAWN (izquierda) -->
    <rect x="0" y="0" width="200" height="400" class="spawn-area" />
    <text class="text" x="40" y="30">Zona de spawn</text>

    <!-- PAREDES DEL RECINTO (desplazado a la derecha por la zona de spawn) -->
    <rect x="220" y="20" width="560" height="360" class="wall" />

    <!-- ENTRADA -->
    <rect x="480" y="20" width="80" height="4" fill="#888" />
    <text class="text" x="490" y="15">Entrada</text>

    <!-- BARRA -->
    <rect x="600" y="60" width="160" height="250" rx="6" class="bar" />
    <text class="text" x="605" y="55">Barra</text>
</svg>
