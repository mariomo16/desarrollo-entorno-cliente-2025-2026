<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 600 400" width="600" height="400">

  <!-- ESTILO GENERAL -->
  <style>
    .wall { fill:none; stroke:#333; stroke-width:4; }
    .table { fill:#8B5E3C; stroke:#6a4226; stroke-width:2; }
    .chair { fill:#4a78c2; stroke:#2e4f80; stroke-width:1.5; }
    .bar    { fill:#444; stroke:#222; stroke-width:2; }
    .text { font-family: sans-serif; font-size: 12px; fill:#333; }
  </style>

  <!-- PAREDES DEL RECINTO -->
  <rect x="20" y="20" width="560" height="360" class="wall"/>

  <!-- ENTRADA -->
  <rect x="260" y="20" width="80" height="4" fill="#888"/>
  <text class="text" x="270" y="15">Entrada</text>

  <!-- BARRA -->
  <rect x="450" y="60" width="100" height="250" rx="6" class="bar"/>
  <text class="text" x="455" y="55">Barra</text>

  <!-- MESAS (cuadradas) + SILLAS -->

  <!-- Mesa 1 -->
  <rect x="80" y="70" width="60" height="60" rx="6" class="table"/>
  <rect x="95" y="50" width="30" height="20" rx="4" class="chair"/>
  <rect x="95" y="130" width="30" height="20" rx="4" class="chair"/>
  <rect x="60" y="90" width="20" height="30" rx="4" class="chair"/>
