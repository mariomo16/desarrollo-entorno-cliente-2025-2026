  <rect x="110" y="110" width="80" height="80" rx="8" ry="8"
        fill="#8B5E3C" stroke="#6a4226" stroke-width="2"/>

  <defs>
    <style>
      .chair { fill: #4a78c2; stroke: #2e4f80; stroke-width: 2; }
    </style>
  </defs>

  <rect class="chair" x="125" y="40" width="50" height="40" rx="6" ry="6"/>

  <rect class="chair" x="125" y="220" width="50" height="40" rx="6" ry="6"/>

